{% extends 'base.html.twig' %}

{% block title %}New Objective{% endblock %}

{% block body %}
    <h1>Create new Objective</h1>
    {% if other_years %}
    <h3> Existing years </h3>
    <ul class="nav">
    {% for year in other_years %}
        <li class="nav-item"> 
            <a class="nav-link" href="{{ path('mbo', {year:year, employee:objective_entry.getForEmployee().id}) }}">
              {{year}} 
            </a>
        </li>
    {% endfor %}
    </ul>
    {% endif %}
    {{ include('objective_entry/_form.html.twig') }}
{% endblock %}
