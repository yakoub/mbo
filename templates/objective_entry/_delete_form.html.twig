<form method="post" action="{{ path('objective_entry_delete', {'id': objective_entry.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" class="container mt-2">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ objective_entry.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>
