{% extends 'base.html.twig' %}

{% block title %}Objective index{% endblock %}

{% block body %}
    <h1>Objective index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Year</th>
                <th>Type</th>
                <th>Subject</th>
                <th>Description</th>
                <th>Weight</th>
                <th>Status</th>
                <th>Score</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for objective_entry in objective_entries %}
            <tr>
                <td>{{ objective_entry.id }}</td>
                <td>{{ objective_entry.year }}</td>
                <td>{{ objective_entry.Type }}</td>
                <td>{{ objective_entry.subject }}</td>
                <td>{{ objective_entry.description }}</td>
                <td>{{ objective_entry.weight }}</td>
                <td>{{ objective_entry.status }}</td>
                <td>{{ objective_entry.score }}</td>
                <td>
                    <a href="{{ path('objective_entry_show', {'id': objective_entry.id}) }}">show</a>
                    <a href="{{ path('objective_entry_edit', {'id': objective_entry.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
