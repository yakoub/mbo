<table class="table"><tbody>
<tr> 
    <th> Employee </th> <td> {{ objective_entry.getForEmployee().FullName }} </td>
</tr>
<tr>
    <th> Manager </th> <td> {{ objective_entry.getByManager().FullName }} </td>
</tr>
</tbody></table>
<div class="container">
{{ form_start(form) }}
    {{ form_errors(form) }}
    <div class="row">
        <div class="col">
            {{ form_row(form.year) }}
            {{ form_row(form.subject) }}
            {{ form_row(form.description) }}
            </div>
        <div class="col">
            {{ form_row(form.Type) }}
            {{ form_row(form.achieve) }}
            {{ form_row(form.weight) }}
        </div>
    </div>
    {{ form_label(form.quad_weight, 'Quadratic weight') }}
    <div class="row">
        <div class="col-3">
            {{ form_row(form.quad_weight.first) }}
        </div>
        <div class="col-3">
            {{ form_row(form.quad_weight.second) }}
        </div>
        <div class="col-3">
            {{ form_row(form.quad_weight.third) }}
        </div>
        <div class="col-3">
            {{ form_row(form.quad_weight.fourth) }}
        </div>
    </div>
    <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_rest(form) }}
{{ form_end(form) }}
</div>
